<script>
    var info = {
              "Select College": ["Select Stream (SS)"],
              "PES": ["Select Stream (SS)",
                      "Mechanical Engineering (ME)",
                      "Computer Science Engineering (CSE)",
                      "Electronics and Communication Engineering (ECE)"],
              "BITS": ["Sorry, we're coming soon to this college (SS)"]
            }
    
    function filload() {
      var college = document.getElementById("college");
      collChoice(college);
    }

    function collChoice(id) {
        var val = id.value;
        $("#branch").empty();
        info[val].forEach(v => {
            newvalue = v.match(/\(([^)]+)\)/)[1];
            $("#branch").append("<option value='" + newvalue + "'>" + v + "</option>");
        });
    }

    function submitValidation() {
      var college = document.getElementById('college').value;
      var branch = document.getElementById('branch').value;
      var sem = document.getElementById('sem').value;
      if(!(college == "Select College" || branch == "SS" || branch == "Select Stream" || sem == "Select Semester")) {
        document.getElementById("filterform").submit();
      }
    }
</script>

