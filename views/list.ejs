<body >
   <style>
    hr{
        border:1px solid rgb(0,0,0,0);
    }
   </style>

    <div style=" margin-left: auto; margin-right: auto;width:100%;background-color: white;">
    <nav class="navbar" style="width:80%;margin: auto;">
        
        <a href="/" class="navbar-brand"><img class="logo" src="https://storage.googleapis.com/swiftprep-web-images/logo.png"></a>
        <% if(!currentUser){ %>
            <a class="nav-link" href="/google">Login</a>
            <% } else { %>
              <div class="dropdown">
                  <button class="btn btn-primary dropdown-toggle" type="button" id="dropdownMenu2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                      Profile
                  </button>
                  <div class="dropdown-menu" aria-labelledby="dropdownMenu2">
                    <button class="dropdown-item" type="button"></button>
                    <div class="dropdown-divider"></div>
                    <a href="/logout" class=" dropdown-item btn">Logout</a>
                  </div>
                </div>
                <% } %>
          </div>
      </nav>


    </div>

<div class="pg3-bg">

    <div class="filterhead" style="text-align: center;margin:0px;"> 
        <div class="filin" style="text-align: center;margin:0px;">
            <h2 id="mainheading" ><b>LEARN EVERYTHING YOU NEED SWIFTLY</b></h2>
        </div>
    </div>
    <% subunique.forEach(function(subU) {%>
        <div data-aos="fade-right" mdata-aos-offset="300" data-aos-easing="ease-in-sine">
            <div class="filtercard">
                <div >
                    <h1><%= subU._id %></h1>
                </div>
                <hr style="border:1px solid white;width:100%;margin-left: 20px;">
                <div class="inside">
                    <% videos.forEach(function(video) {%>
                        <% if(video.Subject == subU._id) {%>
                        <a href="/view/<%= video._id %>">
                            <div id="card" class="cards" >
                                <div style="padding: 5px 8px 0px 8px ;height: 80%;">
                                    <img src="<%= video.Thumbnail %>" width="100%" height="100%">
                                </div>      
                                <div style="padding-left: 10px;"> 
                                    <h2><%= video.Subject %> - <%= video.Chapter %></h2>
                                </div>
                            </div>
                        </a>
                    <% } %>
                <% }) %>
                </div>
            </div>
        </div>
        <br>
    <% }) %>
</div>
  

   <footer id="footer" class="page-footer font-small stylish-color-dark pt-4">
  
        <ul class="list-unstyled list-inline text-center py-2">
          <li class="list-inline-item">
            <h5 class="mb-1">Start your prep!</h5>
          </li>
          <% if(!currentUser){ %>
            <li class="list-inline-item">
              <a href="/google" class="btn btn-primary btn-rounded" style="border-radius: 50px;">Sign up!</a>
            </li>
            <% } else { %>
              <li class="list-inline-item">
                <a href="/filter" class="btn btn-primary btn-rounded" style="border-radius: 50px;">Lets Go!</a>
              </li>
              <% } %>
        </ul>


  
    <hr style="width:70%;background-color:#888;">
    <ul class="list-unstyled list-inline text-center">
      <li class="list-inline-item">
        <a class="btn-floating btn-fb mx-1">
          <i class="fa fa-facebook-f" style="color:white;font-size: 25px;margin-right: 30px;"> </i>
        </a>
      </li>
      <li class="list-inline-item">
        <a class="btn-floating btn-tw mx-1">
          <i class="fa fa-instagram" style="color:white;font-size: 30px;margin-right: 30px;"> </i>
        </a>
      </li>
      <li class="list-inline-item">
        <a class="btn-floating btn-tw mx-1">
          <i class="fa fa-linkedin" style="color:white;font-size: 30px;margin-right: 30px;"> </i>
        </a>
      </li>
    </ul>
  
